<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns="http://java.sun.com/xml/ns/javaee" xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
    xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd"
    id="WebApp_ID" version="3.0">
    <display-name>hessian server</display-name>
<servlet>
		<servlet-name>portal</servlet-name>
		<servlet-class>com.caucho.hessian.server.HessianServlet</servlet-class>
		<init-param>
			<param-name>home-class</param-name>
			<param-value>com.gecpp.fm.ServiceImpl</param-value>
		</init-param>
		<init-param>
			<param-name>home-api</param-name>
			<param-value>com.gecpp.fm.IFuzzySearch</param-value>
		</init-param>
	
		<load-on-startup>1</load-on-startup>
	</servlet>
	<servlet-mapping>
		<servlet-name>portal</servlet-name>
		<url-pattern>/portal</url-pattern>
	</servlet-mapping>
	
	<env-entry>
    	<env-entry-name>fm.param.url</env-entry-name>
    	<env-entry-type>java.lang.String</env-entry-type>
    	<env-entry-value>jdbc:postgresql://192.168.3.33:5432/qeindex</env-entry-value>
    	<!-- <env-entry-value>jdbc:postgresql://localhost:5432/qeindex</env-entry-value> -->
	</env-entry>
	
	<env-entry>
    	<env-entry-name>fm.param.user</env-entry-name>
    	<env-entry-type>java.lang.String</env-entry-type>
    	<env-entry-value>qic_pm</env-entry-value>
    	<!-- <env-entry-value>postgres</env-entry-value> -->
	</env-entry>

	<env-entry>
    	<env-entry-name>fm.param.pwd</env-entry-name>
    	<env-entry-type>java.lang.String</env-entry-type>
    	<env-entry-value>fhqcel!@520</env-entry-value>
    	<!-- <env-entry-value>2xxiiill</env-entry-value> -->
	</env-entry>
	
	<env-entry>
    	<env-entry-name>fm.param.base</env-entry-name>
    	<env-entry-type>java.lang.String</env-entry-type>
    	<env-entry-value>/home/login</env-entry-value>
    	<!--<env-entry-value>C:\Users\dereck\Desktop\Standford</env-entry-value> --> 
	</env-entry>


	</web-app>